{
	"name": "dotink/inkwell-console",
	"description": "Interactive Console for inKWell",
	"keywords": ["cli", "repl"],
	"license": "AGPL-3.0",
	"type": "opus-package",
	"authors": [
		{
			"name": "Matthew J. Sahagian",
			"email": "msahagian@dotink.org",
			"role": "Lead Architect / Developer"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.dotink.org"
